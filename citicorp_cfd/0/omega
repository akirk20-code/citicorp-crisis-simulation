FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 3.2;

boundaryField
{
    inlet
    {
        type            turbulentMixingLengthFrequencyInlet;
        mixingLength    10;
        phi             phi;
        k               k;
        value           uniform 3.2;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 3.2;
        value           uniform 3.2;
    }
    ground
    {
        beta1           0.075;
        blended         false;
        type            omegaWallFunction;
        value           uniform 3.2;
    }
    top
    {
        type            slip;
    }
    sides
    {
        type            symmetry;
    }
    citicorp_tower
    {
        beta1           0.075;
        blended         false;
        type            omegaWallFunction;
        value           uniform 3.2;
    }
    citicorp_stilts
    {
        beta1           0.075;
        blended         false;
        type            omegaWallFunction;
        value           uniform 3.2;
    }
    surroundings
    {
        beta1           0.075;
        blended         false;
        type            omegaWallFunction;
        value           uniform 3.2;
    }
}


