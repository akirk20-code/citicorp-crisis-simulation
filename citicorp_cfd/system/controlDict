stopAt          endTime;
endTime         3000;
deltaT          1;

writeControl    timeStep;
writeInterval   500;
purgeWrite      3;

writeFormat     ascii;
writePrecision  8;
writeCompression off;

timeFormat      general;
timePrecision   6;

runTimeModifiable true;

