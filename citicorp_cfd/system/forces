FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      forces;
}

towerForces
{
    type            forces;
    libs            ("libforces.so");
    patches         (citicorp_tower citicorp_stilts);
    rho             rhoInf;
    rhoInf          1.225;
    CofR            (0 0 124);
    writeControl    timeStep;
    writeInterval   1;
}

towerForceCoeffs
{
    type            forceCoeffs;
    libs            ("libforces.so");
    patches         (citicorp_tower citicorp_stilts);
    rho             rhoInf;
    rhoInf          1.225;
    CofR            (0 0 124);
    liftDir         (0 0 1);
    dragDir         (1 0 0);
    pitchAxis       (0 1 0);
    magUInf         44.7;
    lRef            59.44;
    Aref            14748.8;
    writeControl    timeStep;
    writeInterval   1;
}
